{
  "name": "node-red-contrib-muparser",
  "version": "1.2.6",
  "description": "Blazing-fast mathematical expression evaluator for Node-RED. Native C++ addon using muParser library - 18M+ evaluations/sec. Features: batch processing, custom output locations, dual outputs for error handling, flexible variable sources, and comprehensive function library.",
  "main": "muparser.js",
  "gypfile": true,
  "node-red": {
    "nodes": {
      "muparser": "muparser.js"
    }
  },
  "scripts": {
    "install": "node-gyp rebuild || echo 'Build failed - you may need to run: npm run build'",
    "build": "node-gyp rebuild",
    "clean": "node-gyp clean",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "node-addon-api": "^7.1.0"
  },
  "devDependencies": {
    "node-gyp": "^10.3.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "keywords": [
    "node-red",
    "muparser",
    "math",
    "expression",
    "parser",
    "evaluator",
    "calculator",
    "formula",
    "native",
    "addon",
    "fast",
    "performance",
    "c++",
    "mathematical",
    "computation",
    "real-time",
    "iot",
    "automation",
    "control-systems",
    "signal-processing",
    "batch-processing",
    "trigonometry",
    "scientific"
  ],
  "author": "Cristian Cuevas <cristiancuevas@kirbycorp.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cristiancuevas-kirbycorp/node-red-contrib-muparser.git"
  },
  "bugs": {
    "url": "https://github.com/cristiancuevas-kirbycorp/node-red-contrib-muparser/issues"
  },
  "homepage": "https://github.com/cristiancuevas-kirbycorp/node-red-contrib-muparser#readme"
}
