    "info": "Example flows demonstrating node-red-contrib-muparser capabilities"
  },
  {
    "id": "inject-basic",
    "type": "inject",
    "z": "muparser-examples",
    "name": "Basic: Distance (x=3, y=4)",
    "props": [
      { "p": "payload.x", "v": "3", "vt": "num" },
      { "p": "payload.y", "v": "4", "vt": "num" }
    ],
    "repeat": "",
    "crontab": "",
    "once": false,
    "onceDelay": 0.1,
    "topic": "",
    "x": 170,
    "y": 80,
    "wires": [["muparser-basic"]]
  },
  {
    "id": "muparser-basic",
    "type": "muparser",
    "z": "muparser-examples",
    "name": "sqrt(x^2 + y^2)",
    "expression": "sqrt(x^2 + y^2)",
    "vars": [
      { "name": "x", "value": "payload.x", "valueType": "msg" },
      { "name": "y", "value": "payload.y", "valueType": "msg" }
    ],
    "useExpressionFrom": false,
    "expressionPath": "",
    "expressionPathType": "msg",
    "useVarsFrom": false,
    "varsPath": "",
    "varsPathType": "msg",
    "outputProperty": "payload",
    "outputPropertyType": "msg",
    "batchMode": false,
    "x": 430,
    "y": 80,
    "wires": [["debug-basic"], []]
  },
  {
    "id": "debug-basic",
    "type": "debug",
    "z": "muparser-examples",
    "name": "Result = 5",
    "active": true,
    "tosidebar": true,
    "console": false,
    "tostatus": false,
    "complete": "payload",
    "targetType": "msg",
    "statusVal": "",
    "statusType": "auto",
    "x": 650,
    "y": 80,
    "wires": []
  },
